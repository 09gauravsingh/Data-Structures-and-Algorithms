from typing import List

def contigiousSequence(arr: List[int]) -> int:
  max_sum = arr[0]
  current_sum = arr[0]
  for i in range(1, n):
    current_sum = max(arr[i], current_sum + arr[i])
    max_sum = max(current_sum, max_sum)
  return max_sum  

  
n = int(input())
arr = list(map(int, input().strip().split()))
result = contigiousSequence(arr)
print(result)
