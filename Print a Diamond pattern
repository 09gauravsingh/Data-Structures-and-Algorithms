from typing import List


def patternPrintingII(n: int):
    retList = list()
    for i in range(0, n):
        retList.append('* ' *(i+1))

    for i in range(1, n):
        retList.append('* ' *(n-i))
      

    return retList
# NOTE: Please do not modify this function
def main():
    n = int(input())

    pattern = patternPrintingII(n)

    for i in pattern:
        print(i)

if __name__=="__main__":
    main()
